        "constantgenerator1": {
            "component": "com.sap.util.constantGenerator",
            "metadata": {
                "label": "Generate SQL",
                "x": 17,
                "y": 72,
                "height": 80,
                "width": 120,
                "extensible": true,
                "config": {
                    "counter": 0,
                    "script": "// constants\nvar onceMode = \"once\";\nvar pulseMode = \"pulse\";\nvar triggerMode = \"trigger\";\nvar validModes = [onceMode, pulseMode, triggerMode];\nvar onceActivatedModes = [onceMode, pulseMode];\n\nvar content = $.config.getString(\"content\");\nvar mode = $.config.getString(\"mode\") || onceMode;\nvar counter = $.config.getInt(\"counter\") || 0;\n\nif (validModes.indexOf(mode) == -1) {\n    $.fail(\"Invalid mode '\" + mode + \"'\");\n}\n\nvar isOnceActivatedMode = onceActivatedModes.indexOf(mode) != -1;\n\nfunction emit() {\n    $.out(content);\n}\n\nfunction tick(ctx) {\n    emit();\n}\n\nif (0 === counter) {\n    switch (mode) {\n        case onceMode:\n            $.addGenerator(tick);\n            break;\n        case pulseMode:\n            $.addTimer($.config.getString(\"duration\"), tick);\n            break;\n    }\n}\n\n$.setPortCallback(\"in\", function(ctx, s) {\n    counter--;\n    if (counter <= 0) {\n        if (isOnceActivatedMode) {\n            if (counter === 0) {\n                switch (mode) {\n                    case onceMode:\n                        emit();\n                        break;\n                    case pulseMode:\n                        $.addTimer($.config.getString(\"duration\"), tick);\n                        break;\n                }\n            }\n        } else {\n            emit();\n        }\n    }\n});\n",
                    "content": "select distinct ACTIVITY_ID as CASE_KEY,  to_varchar(localtoutc(CREATED_AT_CET, 'CET'), 'yyyy-mm-dd hh24:mi:ss') as CREATED_ON, case when ACTIVITY_CREATED_BY like_regexpr '^[I|C|D]([0-9]{6,7})$' FLAG 'i' then 'USER' else 'AUTO' end as CREATED_BY, ACTIVITY_PROCESS_TYPE as PROCESS_TYPE, ACTIVITY_STATUS as STATUS, ACTIVITY_INITIATED_BY as INITIATOR, CO_COMPANY_ID as ACCOUNT, CAMP_CAMPAIGN_ID as CAMPAIGN, CAMP_CAMPAIGNTYPE as CAMPAIGN_TYPE, CAMP_OBJECTIVE as CAMPAIGN_SUBTYPE, SALESBAG_LVL3 as SALES_BAG_LVL_3, DISTRIBUTION_CHANNEL_DESCRIPTION as DIS_CHANNEL, ACTIVITY_DIVISION as DIVISION, ACTIVITY_SALES_ORG as SALES_ORG, SALES_SALES_GROUP as SALES_GROUP, CAMP_MARKETING_SEGMENT as MKT_SEGMENT, CO_REGIONAL_BUYING_CLASSIFICATION as GTM_BUY_REG, CO_MASTERCODE as SAP_MASTER, CO_COUNTRY as COUNTRY, SALES_SAP_REGION as REGION, SALES_BU_HUB_CNTRY as SUBREGION, SALES_MU_CNTRY_SUPER_REG as MKT_UNIT, ACTIVITY_REASON as REASON from _SYS_BIC.\"corp.gmar.core.activities/CL_GMAR_ACTIVITIES_AN\" where ACTIVITY_CREATION_DATE >= add_days(add_years(current_date,-2),-dayofyear(add_years(current_date,-2))+1) and ACTIVITY_PROCESS_TYPE = 'ZM01'",
                    "mode": "once"
                }
            }
        },
