        "constantgenerator1": {
            "component": "com.sap.util.constantGenerator",
            "metadata": {
                "label": "Generate SQL",
                "x": 17,
                "y": 72,
                "height": 80,
                "width": 120,
                "extensible": true,
                "config": {
                    "counter": 0,
                    "script": "// constants\nvar onceMode = \"once\";\nvar pulseMode = \"pulse\";\nvar triggerMode = \"trigger\";\nvar validModes = [onceMode, pulseMode, triggerMode];\nvar onceActivatedModes = [onceMode, pulseMode];\n\nvar content = $.config.getString(\"content\");\nvar mode = $.config.getString(\"mode\") || onceMode;\nvar counter = $.config.getInt(\"counter\") || 0;\n\nif (validModes.indexOf(mode) == -1) {\n    $.fail(\"Invalid mode '\" + mode + \"'\");\n}\n\nvar isOnceActivatedMode = onceActivatedModes.indexOf(mode) != -1;\n\nfunction emit() {\n    $.out(content);\n}\n\nfunction tick(ctx) {\n    emit();\n}\n\nif (0 === counter) {\n    switch (mode) {\n        case onceMode:\n            $.addGenerator(tick);\n            break;\n        case pulseMode:\n            $.addTimer($.config.getString(\"duration\"), tick);\n            break;\n    }\n}\n\n$.setPortCallback(\"in\", function(ctx, s) {\n    counter--;\n    if (counter <= 0) {\n        if (isOnceActivatedMode) {\n            if (counter === 0) {\n                switch (mode) {\n                    case onceMode:\n                        emit();\n                        break;\n                    case pulseMode:\n                        $.addTimer($.config.getString(\"duration\"), tick);\n                        break;\n                }\n            }\n        } else {\n            emit();\n        }\n    }\n});\n",
                    "content": "select IC_OBJECT_ID as CASE_KEY, to_varchar(OPP_MGO_CREATION_DATE,'yyyy-mm-dd') || ' 23:59:59' as CREATED_AT, case when AT_IC_OPP_OWNER_PARTNER like_regexpr '^[I|C|D]([0-9]{6,7})|0([0-9]{9})$' FLAG 'i' then 'USER' else 'AUTO' end as CREATED_BY, IC_PROCESS_TYPE as PROCESS_TYPE, IC_STATUS_TXT as STATUS, OPP_MGO_CRM_MARKETING_ELEMENT_ID as CAMPAIGN, ifnull(CAMP_CAMPAIGN_TYPE_TEXT,'') as CAMPAIGN_TYPE, ifnull(CAMP_CAMPAIGN_SUBTYPE_DESC,'') as CAMPAIGN_SUBTYPE, FF_LEVEL_03_DESC as SALES_BAG_LVL_3, FF_LEVEL_04_DESC as SALES_BAG_LVL_4, CA_DIST_CHANNEL_DESC_NB as DIS_CHANNEL, SP_SALES_ORG_TXT as SALES_ORG, ERP_SALES_OFF_TEXT as SALES_OFFICE, ERP_SALES_GRP as SALES_GROUP, IC_INT_MARKET_SEGMENT as MKT_SEGMENT, IC_REG_BUY_CLASS_TXT as GTM_BUY_REG, IC_MASTER_CODE_TXT as SAP_MASTER, IC_ACC_COUNTRY as COUNTRY, CA_NA_GLOBAL_LEVEL_01_DESC as REGION, CA_NA_GLOBAL_LEVEL_02_DESC as SUBREGION, CA_NA_GLOBAL_LEVEL_03_DESC as MKT_UNIT, IC_OPP_REASON_LTXT as REASON, \"Closing_YEAR\" as CLOSING_YEAR, TO_NVARCHAR(\"CM_REVENUE_ACV_kEUR\") as CM_REVENUE_ACV_KEUR, IC_DRM_CATEGORY_TXT, OPP_PIPE_BUILD_TYPE_DESC, OPP_PIPE_BUILD_TYPE_ID, GLOBAL_COUNTRY_DESC as GLOBAL_COUNTRY, OPP_MGO_PRIOR_TRANS_ID as LEAD_ID from _SYS_BIC.\"corp.gmar.datamart/CL_GMAR_GSPI_OPPORTUNITY\" where OPP_MGO_CREATION_DATE >= add_days(add_years(current_date,-2),-dayofyear(add_years(current_date,-2))+1) and CA_PROD_PLATFORM in ('On-Prem', 'Cloud') and OPP_PIPE_BUILD_TYPE_ID = '1'",
                    "mode": "once"
                }
            }
        },
