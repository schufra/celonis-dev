        "constantgenerator1": {
            "component": "com.sap.util.constantGenerator",
            "metadata": {
                "label": "Generate SQL",
                "x": 17,
                "y": 72,
                "height": 80,
                "width": 120,
                "extensible": true,
                "config": {
                    "counter": 0,
                    "script": "// constants\nvar onceMode = \"once\";\nvar pulseMode = \"pulse\";\nvar triggerMode = \"trigger\";\nvar validModes = [onceMode, pulseMode, triggerMode];\nvar onceActivatedModes = [onceMode, pulseMode];\n\nvar content = $.config.getString(\"content\");\nvar mode = $.config.getString(\"mode\") || onceMode;\nvar counter = $.config.getInt(\"counter\") || 0;\n\nif (validModes.indexOf(mode) == -1) {\n    $.fail(\"Invalid mode '\" + mode + \"'\");\n}\n\nvar isOnceActivatedMode = onceActivatedModes.indexOf(mode) != -1;\n\nfunction emit() {\n    $.out(content);\n}\n\nfunction tick(ctx) {\n    emit();\n}\n\nif (0 === counter) {\n    switch (mode) {\n        case onceMode:\n            $.addGenerator(tick);\n            break;\n        case pulseMode:\n            $.addTimer($.config.getString(\"duration\"), tick);\n            break;\n    }\n}\n\n$.setPortCallback(\"in\", function(ctx, s) {\n    counter--;\n    if (counter <= 0) {\n        if (isOnceActivatedMode) {\n            if (counter === 0) {\n                switch (mode) {\n                    case onceMode:\n                        emit();\n                        break;\n                    case pulseMode:\n                        $.addTimer($.config.getString(\"duration\"), tick);\n                        break;\n                }\n            }\n        } else {\n            emit();\n        }\n    }\n});\n",
                    "content": "select distinct VBRK.ERDAT as SD_INVOICE_CREAT_DT, VBRK.ERZET as SD_INVOICE_CREAT_TIME, VBRP.VGBEL as ORDER_ID, VBRK.VBELN as BILLING_DOC_ID ,VBRK.FKART as BILLING_TYPE, VBRK.VTWEG as DISTRIBUTION_CHANNEL,VBRK.BUKRS as COMPANY_CODE,CC.REGION from _SYS_BIC.\"corp.sdsd.invoices/CL_SDSD_VBRK\" VBRK  Join _SYS_BIC.\"corp.sdsd.invoices/CL_SDSD_VBRP\" VBRP on VBRK.VBELN = VBRP.VBELN left outer join ( select CASE when LOCATE(CODE,'>>',-1)>0 then SUBSTR(CODE,LOCATE(CODE,'>>',-1)+2,length(CODE))  else CODE END as COMPANY_CODE, CASE when LOCATE(NAME,'>>',-1)>0 then SUBSTR(NAME,1,LOCATE(NAME,'>>',1)-1)  else NAME END as REGION  from  (SELECT START_RANK,string_agg(PHACOTE58,'>>' order by PHACOLEVE) NAME,string_agg(PHACONONA,'>>' order by PHACOLEVE) CODE from (  SELECT distinct node_id, parent_Id, PHACONONA, PHACOTE57, PHACOTE58,PHACOTE59,PHACOLEVE,START_RANK  FROM     HIERARCHY_ANCESTORS(SOURCE HIERARCHY ( SOURCE ( SELECT rtrim(PHACONOID, '*') AS node_id, rtrim(PHACOPAID, '*') AS parent_id, PHACONONA, PHACOTE57, PHACOTE58,PHACOTE59,PHACOLEVE                             FROM _SYS_BIC.\"corp.mada.hierarchies/CL_CORP_HIER_COMP_CODE_001\"                             ORDER BY parent_id )                   START WHERE PHACOHIOB = '0HIER_NODE'                   DEPTH 100                   ORPHAN IGNORE ) ) Where PHACOLEVE>'03' and HIERARCHY_ROOT_RANK='1') group by START_RANK )) CC on CC.COMPANY_CODE = VBRK.BUKRS  where VBRK.ERDAT>= add_days(add_years(current_date,-2),-dayofyear(add_years(current_date,-2))+1)",
                    "mode": "once"
                }
            }
        },
